<!-- views/agence.ejs -->
<!doctype html>
<html>
	<head>
		<% include header.ejs %>
	</head>
<body>


<div class="container text-center">

	<br><br>
	<h1><span class="fa fa-home"></span><b> Liste des agences </b></h1>

    <br><br>

  <table  class = "table table-hover table-bordered text-center">
    <div class="text-center">
      <tr>
      <th>id agence</th>
      <th>Nom agence</th>
			<th>Code site</th>
      </tr>
  </div>
    <% rows.forEach(function(agences){ %>
      <tr>
        <td> <%=agences.idAgence%></td>
        <td> <%=agences.nom%></td>
				<td> <%=agences.adresse%></td>
      </tr>
      <%})%>
  </table>



</div>


</body>
</html>
